{
  "Logging": {
    "IncludeScopes": false,
    "PathFormat": "Logs/{Date}.log",
    "FileSizeLimitBytes": 104857600, // 100 MB
    "RetainedFileCountLimit": 31, // 31 giorni
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
